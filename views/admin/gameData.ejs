<div class="container">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="text-center">Game List</h1>
    <h4 class="text-center">There are <%= games.length %> in the games database</h1>
    <% games.forEach(function(game) { %>
        <div class='well'>
          <p>Title: <%=  game.name %></p>
          <p>Price: <%=  game.price %></p>
          <% if(game.owned === true){ %>
            <p> This game is owned.</p>
          <% } %>
        </div>
    <% }); %>  
  </div>
</div>
