<h1 class="text-center">Admin Control Panel</h1>

<div class="container">
  <div class="col-md-6 col-md-offset-3">
    <form action="/admin/adminGiveawayList" method="POST" accept-charset="utf-8">
      <div class="form-group">
        <label for="giveawayName">Giveaway Name</label>
        <input type="text" class="form-control" id="giveawayName" name="giveawayName" required>
        <label for="giveawayKeyPhrase">Giveaway Keyphrase</label>
        <input type="text" class="form-control" id="giveawayKeyPhrase" name="giveawayKeyPhrase" required>
      </div>
      <button class="btn btn-success">Submit</button>
    </form>
  </div>
</div>
