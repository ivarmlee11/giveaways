<script src="/js/adminLiveUpdatePlayerList.js"></script>
<script src="/js/timerAdminGameList.js"></script>

<h1 class="text-center">Current Running Giveaways</h1>

<div class="row">

  <div class="col-md-6 col-md-offset-3">
    <% giveaways.forEach(function(giveaway) { %>
      <% if(!giveaway.hidden) { %>
        <div class="well text-center">
          <h3><strong><%=giveaway.name%></strong></h3>
          <h4 data="ids" giveawayId="<%=giveaway.id%>"><%=giveaway.createdAt%></h4>
          <a href="/admin/playerList/<%=giveaway.id%>"><h6>Select This Giveaway</h6></a>
          <span class="numberOfPlayer" giveawayId="<%=giveaway.id%>">
          </span>
          <h3 class="text-center">Player Group</h3>
          <ul class="playerList" playerListId="<%=giveaway.id%>">
          </ul>
          <h3 class="text-center">Winner Group</h3>
          <ul class="playerList" winnerListId="<%=giveaway.id%>">
          </ul>
          <h2 id="timer<%=giveaway.id%>" class="text-center"></h2>
          <h2 id="timerDisplay<%=giveaway.id%>" class="text-center"></h2>  
        </div>
      <% } %>
    <% }); %>
  </div>

</div>
