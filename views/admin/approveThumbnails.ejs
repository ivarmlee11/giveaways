<h1 class="text-center">Thumbnail Approve List</h1>

<div class="container">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="text-center">Approval List</h1>
    <% thumbList.forEach(function(thumb) { %>
	    <div class="well text-center">
	    	<h3 class="text-center"><%= thumb.username %>
	    	</h3>
	    	<div>
	    		<img src="<%= thumb.cloudinary %>"/>
	    	</div>
	      <form action="/avatar/approveList/<%= thumb.id %>" method="POST" accept-charset="utf-8">
	        <button type="submit" class="btn btn-info">Approve</button>
	      </form>
	    </div>
    <% }); %>  
  </div>
</div>
