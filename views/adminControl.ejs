<nav class="navbar navbar-light">
  <ul class="nav navbar-nav">
    <li><a class="nav-link" href="/auth/loggedIn"><h5>Make A Giveaway</h5></a></li>
  </ul>
</nav>

<div class="container">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="text-center">Giveaway Manager</h1>
    <% giveaways.forEach(function(giveaway) { %>
      <div class="well">
        <h3><strong><%=giveaway.name%></strong></h3>
        <a href="/playerList/<%=giveaway.id%>"><h6>Select This Giveaway</h6></a>
      </div>
    <% }); %>

    <h1 id="winner" class="text-center"></h1>
    
  </div>
</div>

