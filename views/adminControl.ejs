<nav class="navbar navbar-default navbar-fixed-top">
  <div class="container">
    <ol class="breadcrumb">
      <li><a href="/auth/loggedIn"><h5>Make A Giveaway</h5></a></li>
    </ol>
  </div>
</nav>

<div class="container">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="text-center">Giveaway Manager</h1>
    <% giveaways.forEach(function(giveaway) { %>
      <div class="well">
        <h3><strong><%=giveaway.name%></strong></h3>
        <a id="winnerId" href="/playerList/<%=giveaway.id%>"><h6>Select This Giveaway</h6></a>
      </div>
    <% }); %>

    <h1 id="winner" class="text-center"></h1>
    
  </div>
</div>

